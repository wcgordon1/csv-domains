---
import { AstroSeo } from "@astrolib/seo";
---

<AstroSeo
  title="csv.domains - Manage and Export Your Domain Portfolio with Ease"
  description="Manage your domains in one place. Bulk upload, set prices, fix errors, and export CSV files ready for Afternic, Spaceship, and SEDO."
  canonical="https://csv.domains/"
  openGraph={{
    url: "https://csv.domains/",
    title: "csv.domains - Manage and Export Your Domain Portfolio with Ease",
    description: "Manage your domains in one place. Bulk upload, set prices, fix errors, and export CSV files ready for Afternic, Spaceship, and SEDO.",
    images: [
      {
        url: "https://csv.domains/og-image.jpg",
        width: 1200,
        height: 630,
        alt: "Open Graph Image Alt",
        type: "image/jpeg",
      },
    ],
    site_name: "csv.domains",
    type: "website",
    locale: "en_US",
  }}
  twitter={{
    handle: "@helloIamWilly",
    site: "@csvdomains",
    cardType: "summary_large_image",
  }}
/>

<!-- Open Graph / Facebook -->
<meta property="og:type" content="website" />
<meta property="og:url" content="https://csv.domains/" />
<meta property="og:title" content="csv.domains - Manage and Export Your Domain Portfolio with Ease" />
<meta
  property="og:description"
  content="Manage your domains in one place. Bulk upload, set prices, fix errors, and export CSV files ready for Afternic, Spaceship, and SEDO."
/>
<meta
  property="og:image"
  content="https://csv.domains/og-image.jpg"
/>

<!-- Twitter -->
<meta property="twitter:card" content="summary_large_image" />
<meta property="twitter:url" content="https://csv.domains/" />
<meta property="twitter:title" content="csv.domains - Manage and Export Your Domain Portfolio with Ease" />
<meta
  property="twitter:description"
  content="Manage your domains in one place. Bulk upload, set prices, fix errors, and export CSV files ready for Afternic, Spaceship, and SEDO."
/>
<meta
  property="twitter:image"
  content="https://csv.domains/og-image.jpg"
/>

<!-- Canonical URL -->
<link rel="canonical" href="https://csv.domains/" />

<!-- Additional SEO -->
<meta name="robots" content="index, follow" />
<meta name="googlebot" content="index, follow" />
